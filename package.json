  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "react-app-env": "^1.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
